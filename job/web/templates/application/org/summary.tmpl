<div id="application-{uuid}" class="mt20 bbd5">

    <div class="rating grey">
        Rating
            <img src="/css/images/star.png" alt="">
            <img src="/css/images/star.png" alt="">
            <img src="/css/images/star.png" alt="">
            <img src="/css/images/star-empty.png" alt="">
            <img src="/css/images/star-empty.png" alt="">
    </div>
    
    <div class="details pt10 grey">
        <span class="ditem d-name b"> <a href="#"> {cvName} </a> </span>
        <span class="ditem d-email"> <a href="mailto:{cvEmail:h}"> {cvEmail} </a> </span>
        <span class="ditem d-phone">  {cvPhone}</span>
        <span class="ditem d-company">  {cvCompany}</span>
        <span class="ditem d-edu">  {cvEducation}</span>
        <span class="ditem d-address">  {cvLocation}</span>
        <span class="link"> <a href="{cvLinkedInPage}">{cvLinkedInPage}</a> </span>
        
    </div>
    
    <div id="application-summary-{uuid}" class="description">
        {summary:h}
        &nbsp; <a href="#" id="{uuid}" class="application-more-link">more&nbsp;&gt;</a>
    </div>
    
     <div id="application-detail-{uuid}" class="hide-me">
        <div class="description">
            {cvDescription:h}
        </div>
        
        <div class="skill">
            {cvSkill}
             &nbsp; <a href="#" id="{uuid}" class="application-less-link">&lt;&nbsp;less</a>
        </div>
        
       
        
    </div>
    
    
    
    <div class="action-links"> 
             <span class="grey-button"> <a href="#" id="{uuid}" class="application-approve-link"> Approve</a> </span>
             &nbsp;
            <span class="grey-button"> <a href="#" id="{uuid}" class="application-reject-link"> Reject</a> </span>
    </div>
        

</div>


