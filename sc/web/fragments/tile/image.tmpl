<div class="tile">
	<div class="photo">         
		<a href="/qa/show.php?id={id}">
			<img src="/{bucket}/{storedName}" width="{width}" height="{height}" title="{originalName}"  alt="{originalName}"/>
        </a> 
    </div>
	
	<div class="description">
		{description}
	</div>
	   
	<div class="author">
		<img src="/nuke/twitter-icon.png" height="32" width="32" alt="icon" /> 		
		<div class="meta">
			<span class="b"> <a href="{userPageURI}">{userName}</a> </span>
			<span class="b">{createdOn}</span>
			<br>
		   <span>&nbsp; </span>
		</div>
	</div>
	
</div>


   
